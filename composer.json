{
  "name": "pollen-solutions/log",
  "description": "Pollen Log Component - Monolog PSR-3 logging implementation.",
  "homepage": "https://github.com/milkcreation/pollen-log.git",
  "keywords": [
    "pollen-solutions",
    "component",
    "logs",
    "logging",
    "PSR-3"
  ],
  "time": "2021-09-15",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/composer": "^2.0",
    "composer/installers": "~1.0",
    "monolog/monolog": "^2.2",
    "pollen-solutions/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Log\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Log\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
