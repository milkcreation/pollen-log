{
  "name": "pollen-solutions/log",
  "description": "Pollen Log Component - Monolog PSR-3 logging implementation.",
  "homepage": "https://www.presstify.com/pollen-solutions/log/",
  "keywords": [
    "pollen-solutions",
    "component",
    "logs",
    "logging",
    "PSR-3"
  ],
  "time": "2021-02-14",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/composer": "^2.0",
    "composer/installers": "~1.0",
    "monolog/monolog": "^2.2",
    "pollen-solutions/container": "^1.0",
    "pollen-solutions/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Log\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Log\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
